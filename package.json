{
    "name": "rawph_b",
    "type": "module",
    "scripts": {
        "dev": "wrangler dev",
        "deploy": "wrangler deploy --minify",
        "cf-typegen": "wrangler types --env-interface CloudflareBindings",
        "db:generate": "drizzle-kit generate",
        "db:migrate:local": "wrangler d1 migrations apply rawph_db --local",
        "db:migrate:remote": "wrangler d1 migrations apply rawph_db --remote",
        "db:studio:local": "drizzle-kit studio --config drizzle.config.local.ts",
        "db:studio:remote": "drizzle-kit studio"
    },
    "dependencies": {
        "hono": "^4.9.9",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20251004.0",
        "@types/node": "^24.7.0",
        "wrangler": "^4.4.0"
    }
}
